<Window x:Class="ScholarStationGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="950" Width="1200">
    <DockPanel Background="#06225e">
        <Frame NavigationUIVisibility ="Hidden" x:Name="_mainFrame" >
            <Frame.Background>
                <LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
                    <GradientStop Color="#FF45EAAE" Offset="0.231"/>
                    <GradientStop Color="#FF5E937F" Offset="0.035"/>
                    <GradientStop Color="#FF00C5FF" Offset="0.129"/>
                </LinearGradientBrush>
            </Frame.Background>
        </Frame>
    </DockPanel>
</Window>
