<Window x:Class="ScholarStationGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="950" Width="1200">
    <DockPanel Background="#06225e">
        <Frame NavigationUIVisibility ="Hidden" x:Name="_mainFrame" >
            <Frame.Background>
                <RadialGradientBrush>
                    <GradientStop Color="#FF45EAAE" Offset="1"/>
                    <GradientStop Color="#FF659E89"/>
                </RadialGradientBrush>
            </Frame.Background>
        </Frame>
    </DockPanel>
</Window>
